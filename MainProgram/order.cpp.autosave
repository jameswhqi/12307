#include "order.h"

//构造函数，用于生成某张票对应的订单 
Order::Order(int target_index = -1)
{
    index = target_index;
}

//给订单中添加一位乘客
void Order::Add_Passenger(const Passenger &new_pass)
{
    pass_list.append(new_pass);
}

//删除指定编号的乘客
void Order::Delete_Passenger(const int target_pass)
{
    pass_list.erase(target_pass);
}
